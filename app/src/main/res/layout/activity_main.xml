<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/txtFechaCompleta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/txtHora"
        android:layout_marginTop="89dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="55dp" />

    <TextView
        android:id="@+id/txtHora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/txtDia"
        android:layout_marginTop="127dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/txtDia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/txtMes"
        android:layout_marginTop="170dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="143dp" />

    <TextView
        android:id="@+id/txtMes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/txtAño"
        android:layout_marginTop="205dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="195dp" />

    <TextView
        android:id="@+id/txtAño"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="116dp"
        android:layout_marginTop="245dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="245dp" />

</RelativeLayout>